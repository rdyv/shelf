{
  "name": "macos",
  "files": {
    "enabled": true,
    "files": [
      "~/.zshrc",
      "~/.zprofile",
      "~/.npmrc",
      "~/.yarnrc",
      "~/.vimrc",
      "~/.config/nvim/init.lua",
      "~/.gitconfig",
      "~/.zsh_history",
      "~/.psql_history",
      "~/.node_repl_history",
      "~/.duckdb_history",
      "~/.clickhouse-client-history",
      "~/.ssh/config",
      "~/.ssh/known_hosts"
    ],
    "directories": [
      "~/.config/alacritty",
      "~/.config/direnv",
      "~/.config/fish",
      "~/.config/gh",
      "~/.config/ghostty",
      "~/.config/git",
      "~/.config/iterm2",
      "~/.config/karabiner",
      "~/.config/kitty",
      "~/.config/nvim",
      "~/.config/tmux",
      "~/.config/zellij",
      "~/.config/htop",
      "~/.config/wireshark",
      "~/.gnupg",
      "~/Library/Preferences/com.apple.dock.plist",
      "~/Library/Preferences/com.apple.finder.plist",
      "~/Library/Preferences/com.apple.Terminal.plist",
      "~/Library/Preferences/com.apple.systempreferences.plist",
      "~/Library/Preferences/com.apple.Safari.plist",
      "~/Library/Preferences/com.apple.controlcenter.plist",
      "~/Library/Preferences/com.apple.MenuMeters.plist",
      "~/Library/Preferences/com.apple.screencapture.plist",
      "~/Library/Preferences/com.apple.desktopservices.plist",
      "~/Library/LaunchAgents"
    ],
    "exclude_patterns": [
      "**/.git/**",
      "**/.svn/**",
      "**/.hg/**",
      "**/node_modules/**",
      "**/target/**",
      "**/build/**",
      "**/dist/**",
      "**/out/**",
      "**/__pycache__/**",
      "**/.pytest_cache/**",
      "**/coverage/**",
      "**/.nyc_output/**",
      "**/.vscode/workspaceStorage/**",
      "**/.idea/workspace.xml",
      "**/.idea/tasks.xml",
      "**/.idea/usage.statistics.xml",
      "**/.idea/shelf/**",
      "**/*.swp",
      "**/*.tmp",
      "**/*~",
      "**/.DS_Store",
      "**/Thumbs.db",
      "**/.localized",
      "**/Icon\r",
      "**/*.log",
      "**/logs/**",
      "**/temp/**",
      "**/tmp/**",
      "**/.cache/**",
      "**/cache/**",
      "**/.docker/buildx/**",
      "**/.docker/cli-plugins/**",
      "**/.Trash/**",
      "**/.fseventsd/**",
      "**/.Spotlight-V100/**",
      "**/.TemporaryItems/**",
      "**/.DocumentRevisions-V100/**",
      "**/venv/**",
      "**/virtualenv/**",
      "**/.venv/**",
      "**/env/**",
      "**/.env/**",
      "**/Vagrantfile",
      "**/*.key",
      "**/*.pem",
      "**/*.p12",
      "**/*.keystore",
      "**/.env.local",
      "**/.env.secret"
    ]
  },
  "homebrew": {
    "enabled": true,
    "brewfile": true,
    "formulas": true,
    "casks": true,
    "services": true,
    "taps": true
  },
  "fonts": {
    "enabled": true,
    "custom_fonts_only": true,
    "system_fonts": false
  },
  "git": {
    "auto_commit": true,
    "commit_message_template": "Backup: {timestamp}",
    "branch": "main"
  },
  "enabled_providers": [
    "files",
    "homebrew",
    "fonts"
  ]
}
