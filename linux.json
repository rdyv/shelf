{
  "name": "linux",
  "files": {
    "enabled": true,
    "files": [
      ".profile",
      ".bashrc",
      ".bash_profile",
      ".zshrc",
      ".gitconfig",
      ".vimrc",
      ".tmux.conf",
      ".inputrc",
      ".ssh/config"
    ],
    "directories": [
      ".ssh",
      ".config",
      ".gnupg",
      ".local/bin"
    ],
    "exclude_patterns": [
      "*.log",
      "*.tmp",
      "__pycache__",
      "node_modules",
      ".git",
      "*.cache",
      ".mozilla/firefox/*/Cache*",
      ".ssh/known_hosts",
      ".ssh/*_rsa",
      ".ssh/*_ed25519"
    ]
  },
  "homebrew": {
    "enabled": false
  },
  "fonts": {
    "enabled": true
  }
}
